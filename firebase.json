{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/projects",
        "destination": "/projects.html"
      },
      {
        "source": "/blogs",
        "destination": "/blogs.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
